{
  "name": "ultimate-backend-website",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/client": "^3.0.0-beta.1",
    "graphql": "^14.5.8",
    "graphql-tag": "^2.10.1",
    "grommet": "^2.7.11",
    "react": "^16.11.0",
    "react-cookie": "^4.0.1",
    "react-document-title": "^2.0.3",
    "react-dom": "^16.11.0",
    "react-hook-form": "^3.26.4",
    "react-router-dom": "^5.1.2",
    "react-scripts-ts": "3.1.0",
    "react-transition-group": "^4.3.0",
    "styled-components": "^5.0.0-rc.1",
    "styled-normalize": "^8.0.6"
  },
  "scripts": {
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
    "g:content": "plop --plopfile .plop/content/plop.config.js",
    "g:component": "plop --plopfile .plop/component/plop.config.js",
    "g:gql": "graphql-codegen --config codegen.yml",
    "deploy-storybook": "storybook-to-ghpages",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn run lint && yarn test -- -u"
    }
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.8.3",
    "@graphql-codegen/fragment-matcher": "1.1.3",
    "@graphql-codegen/typescript": "1.1.3",
    "@graphql-codegen/typescript-graphql-files-modules": "1.1.3",
    "@graphql-codegen/typescript-operations": "1.1.3",
    "@graphql-codegen/typescript-react-apollo": "1.1.3",
    "@storybook/addon-info": "^5.2.5",
    "@storybook/addon-knobs": "^5.2.5",
    "@storybook/react": "^5.2.5",
    "@types/jest": "^24.0.21",
    "@types/node": "^12.12.5",
    "@types/react": "^16.9.11",
    "@types/react-document-title": "^2.0.3",
    "@types/react-dom": "^16.9.3",
    "@types/react-router-dom": "^5.1.2",
    "@types/react-transition-group": "^4.2.3",
    "@types/react-virtualized": "^9.21.5",
    "@types/styled-components": "^4.1.20",
    "@types/thrift": "^0.10.8",
    "plop": "^2.5.2",
    "typescript": "^3.6.4",
    "@storybook/addon-actions": "^4.0.0-alpha.22",
    "@storybook/addon-links": "^4.0.0-alpha.22",
    "@storybook/addon-options": "^3.4.11",
    "@storybook/addons": "^4.0.0-alpha.22",
    "@storybook/storybook-deployer": "^2.3.0",
    "@types/react-test-renderer": "^16.0.2",
    "@types/storybook__addon-actions": "^3.4.1",
    "@types/storybook__addon-info": "^3.4.2",
    "@types/storybook__addon-knobs": "^3.4.1",
    "@types/storybook__addon-options": "^3.2.2",
    "@types/storybook__react": "^3.0.9",
    "cache-loader": "^1.2.2",
    "husky": "^1.0.0-rc.15",
    "jest": "^23.6.0",
    "jest-styled-components": "^7.0.0-beta.1",
    "react-docgen-typescript-loader": "^3.0.0"
  }
}
